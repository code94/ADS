// author:: jackson makale
// eulers problem
// c++ solved

#include <iostream>
using namespace std;

int main()
{
    

    int sum = 0;
    
    
    for (int n =1;n<1000;n++)
    {
        
    
        if (n %3==0 || n %5==0)
        
            sum = sum + n;
       }
    
    cout << sum << endl;
    return(0);
}
